{
  "name": "web-express-auction",
  "version": "0.1.0",
  "repository": "https://github.com/phineasla-hcmus/3.1-Web-Online-Auction",
  "license": "MIT",
  "scripts": {
    "build-node": "tsc",
    "build-sass": "sass public/scss:public/css",
    "build": "npm run build-node && npm run build-sass",
    "start": "node dist/server.js",
    "start-dev": "node -r dotenv/config dist/server.js",
    "watch-node": "nodemon --exec ts-node -T -e ts,hbs -r dotenv/config app/server.ts",
    "watch-sass": "sass --watch public/scss:public/css",
    "watch": "concurrently -k -n \"Sass,Node\" -c \"cyan,blue\" \"npm run watch-sass\" \"npm run watch-node\""
  },
  "dependencies": {
    "@types/express-mysql-session": "^2.1.3",
    "async": "^3.2.2",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "date-fns": "^2.27.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-handlebars": "^6.0.2",
    "express-mysql-session": "^2.1.7",
    "express-session": "^1.17.2",
    "express-validator": "^6.13.0",
    "googleapis": "^92.0.0",
    "handlebars": "^4.7.7",
    "knex": "^0.95.14",
    "moment": "^2.29.1",
    "momentjs": "^2.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "sass": "^1.43.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/async": "^3.2.10",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.10",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-local": "^1.0.34",
    "@types/winston": "^2.4.4",
    "concurrently": "^6.4.0",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
